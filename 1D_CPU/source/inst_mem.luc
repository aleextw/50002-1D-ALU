module inst_mem (
    input ia[16],
    output id[32]
  ) {

const INSTRUCTION = {
b01110111111111111111111111110011,
b01100011000111110000000000000101,
b01100011000111110000000000000101,
b01100011000111110000000000000100,
b01100011000111110000000000000100,
b01100011000111110000000000000011,
b01100011000111110000000000000011,
b01100011000111110000000000000010,
b01100011000111110000000000000010,
b01100011000111110000000000000001,
b01100011000111110000000000000001,
b01100011000111110000000000000000,
b01100011000111110000000000000000
};
    
always {
    id = INSTRUCTION[ia >> 2];
  }
}
